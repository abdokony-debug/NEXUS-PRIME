# النظام الأساسي
NODE_ENV=production
ENCRYPTION_KEY=your_secure_encryption_key_here
LOG_LEVEL=info

# إعدادات المحاكاة
MAX_IDENTITIES_PER_BATCH=50
MAX_REFERRALS_PER_IDENTITY=10
MIN_DELAY_BETWEEN_ACTIONS=2000
MAX_DELAY_BETWEEN_ACTIONS=10000
STEALTH_MODE=advanced
HUMAN_BEHAVIOR_SIMULATION=true

# إعدادات البروكسي
PROXY_ROTATION_ENABLED=true
PROXY_API_KEY=your_proxy_api_key
PROXY_TEST_INTERVAL=3600000
MAX_PROXY_FAILURES=5

# إعدادات البريد
TEMP_EMAIL_SERVICE=tempmail.lol
EMAIL_API_KEY=your_email_api_key
EMAIL_CHECK_INTERVAL=30000

# إعدادات الكابتشا
CAPTCHA_SOLVING_ENABLED=true
CAPTCHA_API_KEY=your_captcha_api_key
CAPTCHA_RETRY_ATTEMPTS=3

# قواعد البيانات
DATABASE_PATH=./databases
BACKUP_INTERVAL=86400000
ENCRYPT_DATABASE=true

# المراقبة والتبليغ
ALERT_ON_FAILURE=true
REPORT_GENERATION_INTERVAL=3600000
METRICS_COLLECTION=true

# حدود الأمان
MAX_CONCURRENT_BROWSERS=5
MEMORY_LIMIT_MB=4096
TIMEOUT_MINUTES=180
RATE_LIMIT_REQUESTS_PER_MINUTE=60
